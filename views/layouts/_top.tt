                                <h6 class="dropdown-header">Voice Acting Guests</h6>
                                [% FOREACH guest IN guestList %]
                                    [% IF guest.enabled == 1 %]
                                        <a class="dropdown-item" role="presentation" href="[% webroot %]/guests/[% guest.pathName %]">[% guest.displayName %]</a>
                                    [% END %]
                                [% END %]
                                <div class="dropdown-divider"></div>
                                <h6 class="dropdown-header">Cultural Guests</h6>
                                [% FOREACH cultureGuest IN culturalGuestList %]
